{
  "name": "demo",
  "version": "1.0.0",
  "description": "best boilerplate",
  "main": "src/index.js",
  "keywords": [
    "webpack"
  ],
  "author": "addamx",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development node server.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.prod.config.js",
    "clean": "rm -rf dist/*",
    "lint": "eslint src/**/*.js*",
    "lint:style": "stylelint \"src/**/*.less\"",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "fix-style": "prettier-stylelint --write 'src/**/*.{css,less,scss}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix src/**/*.js",
      "git add"
    ],
    "src/**/*.less": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
