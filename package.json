    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "eventsource-polyfill": "^0.9.6",
